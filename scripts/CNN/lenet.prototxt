layers {
  name: "ip"
  type: INNER_PRODUCT
  bottom: "conv5"
  top: "ip_zw"
  inner_product_param {
    num_output: 361
    weight_filler {
      type: "xavier"
      }
    bias_filler {
      type: "constant"
      }
   }
}



#only prediction
layers {
  name: "softmax"
  type: SOFTMAX
  bottom: "ip_zw"
  top: "ip"
}

